{
    "manifest_version": 3,
    "name": "Minimal Manifest",
    "version": "1.0.0",
    "description": "A basic example extension with only required keys",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "icons": {
        "48": "icon-32.png"
    }
}